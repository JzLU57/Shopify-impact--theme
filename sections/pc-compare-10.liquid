<style>
  .section-pc-{{section.id}} {
      max-width: {% if full_width %} 100% {% else %} 1200px {% endif %};
      {% if full_border %}
          border: {{section_border_w}}px solid {{section_border_color}};
      {% endif %}
      border-radius: {{section_radius}}px;
      margin: 0 auto;
      margin-top: {{section_mt}}px !important;
      margin-bottom: {{section_mb}}px !important;
     background-color: {{section_bg}};
      overflow: hidden;
  }

  .section-pc-{{section.id}}-settings {
      margin: 0 auto;
      padding-top: 27px;
      padding-bottom: 27px;
      padding-left: 1.5rem;
      padding-right: 1.5rem;
  }

  .comparison-heading-pc-{{section.id}} {
      text-align: center;
  }

  .comparison-heading-pc-{{section.id}} * {
      margin: 0px;
      font-size: {{ heading_size_mobile }}px;
      font-weight: {{ heading_weight }};
      color: {{ heading_color }};
      line-height: 150%;
      text-transform: unset;
  }

  .comparison-body-pc-{{section.id}} {
      display: flex;
      align-items: center;
      flex-wrap: wrap;
      margin-top: 30px;
      gap: 10px;
      position: relative;
  }

  .comparison-versus-icon-pc-{{section.id}} {
      position: absolute;
      left: 50%;
      top: 14%;
      transform: translateX(-50%);
      width: {{vs_w_mobile}}px;
      overflow: hidden;
  }

  .comparison-versus-icon-pc-{{section.id}} img,
  .comparison-versus-icon-pc-{{section.id}} svg {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }

  .comparison-versus-icon-pc-{{section.id}} svg circle {
      fill: {{vs_bg}};
  }

  .comparison-versus-icon-pc-{{section.id}} svg path {
      fill: {{vs_color}};
  }

  .comparison-image-pc-{{section.id}} {
      flex: 0 1 calc(50% - (10px / 2));
      overflow: hidden;
      border-radius: {{img_radius}}px;
      border: {{img_border_w}}px solid {{img_border_color}};
  }

  .comparison-image-pc-{{section.id}} img,
  .comparison-image-pc-{{section.id}} svg {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }


  .comparison-image-pc-{{section.id}} .img {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }

  .comparison-image-pc-{{section.id}} svg {
      background: #AFAFAF;
  }

  .comparison-table-pc-{{section.id}} {
      flex: 1 1 auto;
      order: 3;
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 6px;
  }

  .comparison-value-pc-{{section.id}} {
      display: flex;
      gap: 5px;
      width: 100%
  }

  .comparison-right-value-pc-{{section.id}},
  .comparison-left-value-pc-{{section.id}} {
      display: flex;
      align-items: center;
      justify-content: center;
      gap: 10px;
      background: {{compa_bg}};
      border: {{compa_border_w}}px solid {{compa_border_color}};
      padding: 16px 8px;
      border-radius: {{compa_radius}}px;
      width: 100%;
  }

  .comparison-icon-pc-{{section.id}} {
      width: {{compa_icon_w_mobile}}px;
      height: {{compa_icon_w_mobile}}px;
      flex-shrink: 0;
  }

  .comparison-icon-pc-{{section.id}} img,
  .comparison-icon-pc-{{section.id}} svg {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }

  .comparison-text-pc-{{section.id}} {
      margin: 0px;
      font-size: {{compa_size_mobile}}px;
      color: {{compa_color}};
      line-height: 160%;
      text-transform: unset;
      text-align: center;
      text-decoration: none;
  }

  .comparison-button-wrapper-pc-{{section.id}} {
      display: flex;
      align-items: center;
      justify-content: center;
  }

  .comparison-button-pc-{{section.id}} {
      display: flex;
      align-items: center;
      justify-content: center;
      max-width: fit-content;
      gap: 10px;
      width: 100%;
      margin: 0;
      margin-top: 20px;
      font-size: {{ btn_size_mobile }}px;
      color: {{ btn_color }};
      line-height: 100%;
      text-align: center;
      text-transform: unset;
      text-decoration: none;
      padding: 12px 32px;
      border-radius: {{ btn_radius }}px;
      transition: all 0.25s ease 0s;
      background-color: transparent;
      border: 0px;
      cursor: pointer;
      font-weight: {{ btn_weight }};
  }

  .comparison-button-inner-pc-{{section.id}} {
      margin: 0px;
      display: flex;
      align-items: center;
      justify-content: center;
      gap: 10px;
  }

  .comparison-button-pc-{{section.id}}:hover {
      color: {{ btn_color_hover }};
      transition: all 0.25s ease 0s;
  }

  .comparison-button-pc-{{section.id}} svg {
      width: 14px;
      height: 14px;
  }

  .comparison-button-pc-{{section.id}} svg path {
      fill: {{ btn_color }};
      transition: all 0.25s ease 0s;
  }

  .comparison-button-pc-{{section.id}}:hover svg path {
      fill: {{ btn_color_hover }};
      transition: all 0.25s ease 0s;
  }

  @media(min-width: 1024px) {

      .section-pc-{{section.id}}-settings {
          padding: 0 5rem;
          padding-top: 36px;
          padding-bottom: 36px;
          padding-left: 2rem;
          padding-right: 2rem;
      }

      .comparison-heading-pc-{{section.id}} {
          text-align: center;
      }

      .comparison-heading-pc-{{section.id}} * {
          font-size: {{ heading_size }}px;
      }

      .comparison-body-pc-{{section.id}} {
          flex-wrap: nowrap;
          gap: 16px;
          margin-top: 30px;
      }

      .comparison-table-pc-{{section.id}} {
          gap: 6px;
      }

      .comparison-image-pc-{{section.id}} {
          flex: 0 1 320px;
      }

      .comparison-table-pc-{{section.id}} {
          order: unset;
      }

      .comparison-right-value-pc-{{section.id}},
      .comparison-left-value-pc-{{section.id}} {
          padding: 18px 16px;
      }

      .comparison-icon-pc-{{section.id}} {
          width: {{compa_icon_w}}px;
          height: {{compa_icon_w}}px;
      }

      .comparison-text-pc-{{section.id}} {
          font-size: {{compa_size}}px;
      }

      .comparison-versus-icon-pc-{{section.id}} {
          top: 50% !important;
          transform: translate(-50%, -50%);
          width: {{vs_w}}px;
      }

      .comparison-button-wrapper-pc-{{section.id}} {
          justify-content: center;
      }

      .comparison-button-pc-{{section.id}} {
          margin-top: 28px;
          padding: 12px 32px;
          font-size: {{ btn_size }}px;
      }

  }


  .section-pc-{{section.id}}-settings {
      max-width: 1300px;
  }


  .comparison-heading-pc-{{section.id}} * {
      font-style: normal;
  }


  .comparison-text-pc-{{section.id}} {
      font-weight: {{compa_weight}};
      font-style: normal;
  }

  .comparison-image-pc-{{section.id}} {
      aspect-ratio: 12/12;
  }

  .comparison-versus-icon-pc-{{section.id}} {
      top: 14%;
  }


  @media(min-width: 1024px) {
      .comparison-image-pc-{{section.id}} {
          aspect-ratio: 12/12;
      }
  }


  .comparison-button-pc-{{section.id}} {
      background-color: {{ btn_bg }};
      border: {{ btn_border_w }}px solid {{ btn_border_color }};
  }

  .comparison-button-pc-{{section.id}}:hover {
      background-color: {{ btn_bg_hover }};
      border: {{ btn_border_w }}px solid {{ btn_border_color_hover }};
  }
</style>

<div class="section-pc-{{section.id}} comparison-pc-{{section.id}}">
  <div class="section-pc-{{section.id}}-settings">
    <div class="comparison-heading-pc-{{section.id}}">
      <h2>{{ heading }}</h2>
    </div>

    <div class="comparison-body-pc-{{section.id}}">
      <div class="comparison-versus-icon-pc-{{section.id}}">
        <svg xmlns="http://www.w3.org/2000/svg" width="73" height="73" viewBox="0 0 73 73" fill="none">
