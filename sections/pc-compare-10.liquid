                      fill="white"></path>
                  <path id="Vector_2"
                      d="M17.5781 28.7793H22.4314L27.5274 39.8978L32.6014 28.7793H37.4106L29.8438 44.2217H25.1228L17.5781 28.7793Z"
                      fill="white"></path>
              </g>
          </g>
        </svg>
      </div>
      <div class="comparison-image-pc-{{section.id}}">
        {% if left_img != blank %}
          <img
            src="{{ left_img | img_url:'master' }}"
            alt="Image_left"
          >
        {% else %}
          <div class="img">
            {{ 'product-1' | placeholder_svg_tag: 'placeholder_svg' }}
          </div>
        {% endif %}
      </div>
      <div class="comparison-table-pc-{{section.id}}">
        {% for block in section.blocks %}
          <div class="comparison-value-pc-{{section.id}}">
            <div class="comparison-left-value-pc-{{section.id}}">
              <div class="comparison-icon-pc-{{section.id}}">
                {{ block.settings.left_icon }}
              </div>

              <p class="comparison-text-pc-{{section.id}}">
                {{ block.settings.left_text }}
              </p>
            </div>
            <div class="comparison-right-value-pc-{{section.id}}">
              <div class="comparison-icon-pc-{{section.id}}">
                {{ block.settings.right_icon }}
              </div>

              <p class="comparison-text-pc-{{section.id}}">
                {{ block.settings.right_text }}
              </p>
            </div>
          </div>
        {% endfor %}
      </div>
      <div class="comparison-image-pc-{{section.id}}">
        {% if right_img != blank %}
          <img
            src="{{ right_img | img_url:'master' }}"
            alt="Image_right"
          >
        {% else %}
          <div class="img">
            {{ 'product-1' | placeholder_svg_tag: 'placeholder_svg' }}
          </div>
        {% endif %}
      </div>
    </div>
    <div class="comparison-button-wrapper-pc-{{section.id}}">
      {% if btn_text != blank %}
        <a
          href="{{ btn_link }}"
          class="comparison-button-pc-{{section.id}}"
        >
          <p class="comparison-button-inner-pc-{{section.id}}">
            {{ btn_text }}
          </p>
        </a>
      {% endif %}
    </div>
  </div>
</div>

