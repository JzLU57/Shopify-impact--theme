<style>
  .section-pc-{{section.id}} {
      max-width: {% if full_width %} 100% {% else %} 1200px {% endif %};
      {% if full_border %}
          border: {{section_border_w}}px solid {{section_border_color}};
      {% endif %}
      border-radius: {{section_radius}}px;
      margin: 0 auto;
      margin-top: {{section_mt}}px !important;
      margin-bottom: {{section_mb}}px !important;
     background-color: {{section_bg}};
  }

  .section-pc-{{section.id}}-settings {
      margin: 0 auto;
      padding-top: 27px;
      padding-bottom: 27px;
      padding-left: 1.5rem;
      padding-right: 1.5rem;
  }

  .comparison-heading-pc-{{section.id}} {
      text-align: center;
  }

  .comparison-heading-pc-{{section.id}} * {
      margin: 0;
      font-size: {{heading_size_mobile}}px;
      color: {{heading_color}};
      line-height: 110%;
      text-transform: unset;
  }

  .comparison-body-pc-{{section.id}} {
      margin-top: 32px;
      display: flex;
      align-items: start;
      gap: 16px;
      flex-direction: column;
  }

  .comparison-column-pc-{{section.id}} {
      padding: 20px 10px;
      border-radius: {{tab_radius}}px;
      border: {{tab_border_w}}px solid {{tab_border_color}};
      background-color: {{tab_bg2}};
      width: 100%;
  }

  .comparison-column-pc-{{section.id}}.active {
      background-color: {{tab_bg1}};
  }

  .comparison-column-desktop-pc-{{section.id}} {
      display: none;
  }

  .comparison-product-heading-pc-{{section.id}} {
      text-align: left;
      padding-bottom: 12px;
      border-bottom: {{tab_inside_border_w}}px solid {{tab_inside_border_color}};
      padding-left: 10px;
  }

  .comparison-product-heading-pc-{{section.id}} * {
      margin: 0;
      font-size: {{tab_heading_size_mobile}}px;
      color: {{tab_heading_color}};
      line-height: 130%;
      text-transform: unset;
  }

  .comparison-item-pc-{{section.id}} {
      padding: 10px 10px;
      border-bottom: {{tab_inside_border_w}}px solid {{tab_inside_border_color}};
  }

  .comparison-top-pc-{{section.id}} {
      display: flex;
      align-items: center;
      gap: 8px;
  }

  .comparison-icon-pc-{{section.id}} {
      flex: 0 0 12px;
      height: {{ tab_icon_w_mobile }}px;
  }

  .comparison-icon-pc-{{section.id}} svg,
  .comparison-icon-pc-{{section.id}} img {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: contain;
  }

  .comparison-title-pc-{{section.id}} {
      text-align: left;
      margin: 0;
      font-size: {{tab_title_size_mobile}}px;
      color: {{tab_title_color}};
      line-height: 130%;
      text-transform: unset;
      font-weight: {{tab_title_weight}};
  }

  .comparison-text-pc-{{section.id}} {
      display: block;
      width: 100%;
      text-align: left;
      margin: 0;
      margin-top: 4px;
      padding-left: 20px;
      font-size: {{tab_desc_size_mobile}}px;
      color: {{tab_desc_color}};
      line-height: 130%;
      text-transform: unset;
  }

  .comparison-button-pc-{{section.id}} {
      display: flex;
      align-items: center;
      justify-content: center;
      gap: 10px;
      width: 100%;
      max-width: 200px;
      margin: 0;
      margin-top: 16px;
      font-size: {{tab_btn_size_mobile}}px;
      color: {{tab_btn_color}};
      line-height: 100%;
      text-align: center;
      text-transform: unset;
      text-decoration: none;
      padding: 16px 24px;
      border-radius: {{tab_btn_radius}}px;
      transition: all 0.25s ease 0s;
      position: relative;
      font-weight: {{tab_btn_weight}};
      border:{{tab_btn_border_w}}px solid {{tab_btn_border_color}};
      z-index: 2;
  }

  .comparison-button-pc-{{section.id}}:hover {
      color: {{tab_btn_color_hover}};
      border:{{tab_btn_border_w}}px solid {{tab_btn_border_color_hover}};
      transition: all 0.25s ease 0s;
  }

  .comparison-button-pc-{{section.id}} svg {
      width: 14px;
      height: 14px;
  }

  .comparison-button-pc-{{section.id}} svg path {
      fill: {{tab_btn_color}};
      transition: all 0.25s ease 0s;
  }

  .comparison-button-pc-{{section.id}}:hover svg path {
      fill: {{tab_btn_color_hover}};
      transition: all 0.25s ease 0s;
  }

  @media(min-width: 1024px) {

      .section-pc-{{section.id}} {
          margin-top: 0px;
          margin-bottom: 0px;
      }

      .section-pc-{{section.id}}-settings {
          padding: 0 5rem;
          padding-top: 36px;
          padding-bottom: 36px;
          padding-left: 5rem;
          padding-right: 5rem;
      }

      .comparison-heading-pc-{{section.id}} {
          text-align: center;
      }

      .comparison-heading-pc-{{section.id}} * {
          font-size: {{heading_size}}px;
      }

      .comparison-body-pc-{{section.id}} {
          margin-top: 64px;
          gap: 32px;
          flex-direction: row;
      }

      .comparison-column-pc-{{section.id}} {
          flex: 1 0;
      }

      .comparison-column-desktop-pc-{{section.id}} {
          display: block;
      }

      .comparison-column-pc-{{section.id}} {
          padding: 20px 20px;
      }

      .comparison-product-heading-pc-{{section.id}} {
          text-align: left;
          padding-left: 10px;
          padding-bottom: 16px;
      }

      .comparison-product-heading-pc-{{section.id}} * {
          font-size: {{tab_heading_size}}px;
      }

      .comparison-item-pc-{{section.id}} {
          padding: 10px 10px;
      }

      .comparison-icon-pc-{{section.id}} {
          flex: 0 0 12px;
          height: {{ tab_icon_w}}px;
      }

      .comparison-title-pc-{{section.id}} {
          text-align: left;
          font-size: {{tab_title_size}}px;
      }

      .comparison-text-pc-{{section.id}} {
          text-align: left;
          padding-left: 20px;
          font-size:{{tab_desc_size}}px;
      }

      .comparison-button-pc-{{section.id}} {
          margin-top: 16px;
          max-width: 200px;
          padding: 16px 24px;
          font-size: {{tab_btn_size}}px;
      }
  }


  .section-pc-{{section.id}}-settings {
      max-width: 120rem;
  }


  .comparison-heading-pc-{{section.id}} * {
      font-weight: {{heading_weight}};
      font-style: normal;
  }


  .comparison-product-heading-pc-{{section.id}} * {
      font-weight: {{tab_heading_weight}};
      font-style: normal;
  }


  .comparison-title-pc-{{section.id}} {
      font-style: normal;
  }


  .comparison-text-pc-{{section.id}} {
      font-weight: 400;
      font-style: normal;
  }


  .comparison-button-pc-{{section.id}} {
      font-style: normal;
  }


  @media(min-width: 1024px) {
      .comparison-top-pc-{{section.id}} {
          justify-content: start;
      }
  }


  .comparison-button-pc-{{section.id}} {
      margin-left: auto;
      margin-right: auto;
  }


  @media(min-width: 1024px) {
      .comparison-button-pc-{{section.id}} {
          margin-left: auto !important;
          margin-right: auto !important;
      }
  }


  .comparison-button-pc-{{section.id}} {
      background-color: {{tab_btn_bg}};
  }

  .comparison-button-pc-{{section.id}}:hover {
      background-color: {{tab_btn_bg_hover}};
  }
</style>

<div class="section-pc-{{section.id}} comparison-pc-{{section.id}}">
  <div class="section-pc-{{section.id}}-settings">
    <div class="comparison-heading-pc-{{section.id}}">
      <h2>
        {{ heading }}
      </h2>
    </div>

    <div class="comparison-body-pc-{{section.id}}">
      <div class="comparison-column-pc-{{section.id}} active ">
        <div class="comparison-product-heading-pc-{{section.id}}">
          <h3>{{ tab_heading1 }}</h3>
        </div>

        {% for block in section.blocks %}
          {% if block.type == 'first_table_item' %}
            <div class="comparison-item-pc-{{section.id}}">
              <div class="comparison-top-pc-{{section.id}}">
                <div class="comparison-icon-pc-{{section.id}}">
                  {{ block.settings.icon }}
                </div>
                <p class="comparison-title-pc-{{section.id}}">
                  {{ block.settings.title }}
                </p>
              </div>

              <p class="comparison-text-pc-{{section.id}}">
                {{ block.settings.desc }}
              </p>
            </div>
          {% endif %}
        {% endfor %}

        {% if tab1_btn_text != blank %}
          <a
            href="{{ tab1_btn_link }}"
            class="comparison-button-pc-{{section.id}}"
          >
            {{ tab1_btn_text }}
          </a>
        {% endif %}
      </div>

      <div class="comparison-column-pc-{{section.id}}  ">
        <div class="comparison-product-heading-pc-{{section.id}}">
          <h3>{{ tab_heading2 }}</h3>
        </div>

        {% for block in section.blocks %}
          {% if block.type == 'second_table_item' %}
            <div class="comparison-item-pc-{{section.id}}">
              <div class="comparison-top-pc-{{section.id}}">
                <div class="comparison-icon-pc-{{section.id}}">
                  {{ block.settings.icon }}
                </div>
                <p class="comparison-title-pc-{{section.id}}">
                  {{ block.settings.title }}
                </p>
              </div>

              <p class="comparison-text-pc-{{section.id}}">
                {{ block.settings.desc }}
              </p>
            </div>
          {% endif %}
        {% endfor %}

        {% if tab2_btn_text != blank %}
          <a
            href="{{ tab2_btn_link }}"
            class="comparison-button-pc-{{section.id}}"
          >
            {{ tab2_btn_text }}
          </a>
        {% endif %}
      </div>
    </div>
  </div>
</div>

