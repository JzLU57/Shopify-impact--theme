      display: block;
      width: 100%;
      height: 100%;
      object-fit: content;
  }

  .hero-feature-icon-feature .img{
      display: block;
      width: 100%;
      height: 100%;
      object-fit: content;
  }

  .hero-feature-icon-feature svg path {
      fill: {{feat_text_color}};
  }

  @media (min-width: 1024px) {

