      .section-pc-{{section.id}}-settings {
                padding: 0 5rem;
                padding-top: 100px;
                padding-bottom: 100px;
                padding-left: 5rem;
                padding-right: 5rem;
                height: auto;
            }

      .hero-body-pc-{{section.id}} {
          gap: 52px;
      }

      .hero-content-pc-{{section.id}} {
          justify-content: center;
          align-items: start;
      }

      .hero-image-mobile-pc-{{section.id}} {
          display: none !important;
      }

      .hero-image-desktop-pc-{{section.id}} {
          display: block !important;
      }
  }

  .section-pc-{{section.id}}-settings {
      max-width:  {% if full_width %} 100% {% else %} 1200px {% endif %};
  }


  .hero-pc-{{section.id}} {
      background-size: cover;
      background-repeat: no-repeat;
  }


