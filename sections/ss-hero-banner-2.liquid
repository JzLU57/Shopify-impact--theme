      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      z-index: -1;
      overflow: hidden;
  }

  .hero-image-pc-{{section.id}}::after {
      position: absolute;
      content: "";
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: {{section_overlay}}66;
      z-index: 0;
    }

  .hero-image-pc-{{section.id}} img,
  .hero-image-pc-{{section.id}} svg {
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }

  .hero-image-pc-{{section.id}} .img{
      display: block;
      width: 100%;
      height: 100%;
      object-fit: cover;
  }

  .hero-image-pc-{{section.id}} svg {
      background-color: #AFAFAF;
  }

  .hero-body-pc-{{section.id}} {
      display: grid;
      gap: 24px;
  }

  .hero-content-pc-{{section.id}} {
      display: flex;
      flex-direction: column;
      justify-content: center;
      align-items: center;
      background-color: ;
      border-radius: px;
      grid-area: content;
  }




