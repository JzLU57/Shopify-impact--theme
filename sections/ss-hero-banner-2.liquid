          margin-top: 28px;
          gap: 2em;
      }

      .hero-feature-feature {
          display: flex;
          gap: 12px;
      }

      .hero-feature-icon-feature {
          height: {{feat_img_w}}px;
          width: {{feat_img_w}}px;
      }

      .hero-feature-text-feature {
          font-size: {{feat_text_size}}px;
      }
