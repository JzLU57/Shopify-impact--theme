      display: flex;
      align-items: center;
      justify-content: center;
      gap: 10px;
      text-decoration: none;
  }

  .hero-button-button_cyVra9:hover {
      color: {{ btn_color_hover }};
      transition: all 0.25s ease 0s;
  }

