          text-align: left;
      }

      .hero-heading-heading_8E4KrG * {
          font-size: {{ heading_size }}px;
      }
  }


  .hero-text-text_cppebz {
      margin-top: 8px;
      text-align: center;
  }

  .hero-text-text_cppebz * {
      margin: 0;
      font-size: {{ sub_heading_size_mobile }}px;
      color: {{ sub_heading_color }};
      line-height: 170%;
      text-transform: unset;
  }

  @media(min-width: 1024px) {

