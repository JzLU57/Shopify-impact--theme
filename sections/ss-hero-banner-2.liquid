<div class="section-pc-{{section.id}} hero-pc-{{section.id}}">
  <div class="section-pc-{{section.id}}-settings">
    <div class="hero-image-pc-{{section.id}}">
      {% if hero_img != blank %}
        <img
          src="{{ hero_img | img_url:'master' }}"
          alt="Hero Image"
        >
      {% else %}
        <div class="img">
          {{ 'product-1' | placeholder_svg_tag: 'placeholder_svg' }}
        </div>
      {% endif %}
    </div>
    <div class="hero-body-pc-{{section.id}}">
      <div class="hero-content-pc-{{section.id}}">
        <div class="hero-review-review_RPMgbt">
          <div class="hero-stars-review_RPMgbt">
            {% assign full_stars = reviews_note | floor %}
            {% assign partial_star = reviews_note | minus: full_stars | ceil %}
            {% assign empty_stars = 5 | minus: full_stars | minus: partial_star %}

            {% for i in (1..full_stars) %}
              <svg xmlns="http://www.w3.org/2000/svg" width="20" height="19" viewBox="0 0 20 19" fill="none">
                <path d="M10 0L12.9389 5.95492L19.5106 6.90983L14.7553 11.5451L15.8779 18.0902L10 15L4.12215 18.0902L5.24472 11.5451L0.489435 6.90983L7.06107 5.95492L10 0Z" fill="{{reviews_star_color}}"></path>
              </svg>
            {% endfor %}
            {% if partial_star > 0 %}
              <svg
                xmlns="http://www.w3.org/2000/svg"
                width="20"
                height="19"
                viewBox="0 0 20 19"
                fill="none"
              >
                <defs>
                    <linearGradient id="grad{{section.id}}">
                        <stop offset="0%" stop-color="{{reviews_star_color}}"></stop>
                        <stop offset="50%" stop-color="{{reviews_star_color}}"></stop>
                        <stop offset="50%" stop-color="transparent"></stop>
                        <stop offset="100%" stop-color="transparent"></stop>
                    </linearGradient>
                </defs>

                <path stroke="{{reviews_star_color}}"
                    d="M10 0L12.9389 5.95492L19.5106 6.90983L14.7553 11.5451L15.8779 18.0902L10 15L4.12215 18.0902L5.24472 11.5451L0.489435 6.90983L7.06107 5.95492L10 0Z"
                    fill="url(#grad{{section.id}})">
                </path>
              </svg>
            {% endif %}

            {% if empty_stars > 0 %}
              {% for i in (1..empty_stars) %}
                <svg
                  xmlns="http://www.w3.org/2000/svg"
                  width="20"
                  height="19"
                  viewBox="0 0 20 19"
                  fill="none"
                >
                  <path d="M10 0L12.9389 5.95492L19.5106 6.90983L14.7553 11.5451L15.8779 18.0902L10 15L4.12215 18.0902L5.24472 11.5451L0.489435 6.90983L7.06107 5.95492L10 0Z" fill="transparent" stroke="{{reviews_star_color}}"></path>
                </svg>
              {% endfor %}
            {% endif %}
          </div>

          <div class="hero-review-text-review_RPMgbt">
            <p>
              {{ reviews_note }}/5 . {{ reviews_numbers }}
              {{ reviews_text }}
            </p>
          </div>
        </div>

        <div class="hero-heading-heading_8E4KrG">
          <h1>{{ heading }}</h1>
        </div>

        <div class="hero-text-text_cppebz">
          {{ sub_heading }}
        </div>

        {% if btn_text != blank %}
          <a href="{{ btn_link }}" class="hero-button-button_cyVra9">
            <p class="hero-button-inner-button_cyVra9">{{ btn_text }}</p>
          </a>
        {% endif %}

        <div class="hero-features">
          {% for block in section.blocks %}
            {% if block.type == 'feature' %}
              <div class="hero-feature-feature">
                <div class="hero-feature-icon-feature">
                  {% if block.settings.feature_icon %}
                    <img src="{{ block.settings.feature_icon | img_url: 'master' }}" alt="Image_{{ block.id }}">
                  {% else %}
                    <div class="img">
                      {{ 'product-1' | placeholder_svg_tag: 'placeholder_svg' }}
                    </div>
                  {% endif %}
                </div>
                <p class="hero-feature-text-feature">{{ block.settings.feature_text }}</p>
              </div>
            {% endif %}
          {% endfor %}
        </div>
      </div>
    </div>
  </div>
</div>

